{
    "name": "michielkempen/laravel-actions",
    "keywords": [
        "michielkempen",
        "laravel-actions"
    ],
    "homepage": "https://github.com/michielkempen/laravel-actions",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Kempen",
            "email": "kempenmichiel@gmail.com",
            "homepage": "https://michielkempen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "michielkempen/laravel-enum": "^1.0",
        "michielkempen/laravel-uuid-model": "^2.0",
        "spatie/async": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8"
    },
    "autoload": {
        "psr-4": {
            "MichielKempen\\LaravelActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichielKempen\\LaravelActions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichielKempen\\LaravelActions\\ActionsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
